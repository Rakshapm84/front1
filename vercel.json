{
  "version": 2,
  "builds": [
    {
      "src": "src/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:png|jpg|jpeg|gif|svg|webp|ico))",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*\\.(?:css|js))",
      "dest": "/src/$1"
    },
    {
      "src": "^/$",
      "dest": "/src/boot1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/boot1.html"
    }
  ]
}
